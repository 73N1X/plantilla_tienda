{
    "name": "masia/fpsgaming",
    "description": "Shop",
    "type": "project",
    "autoload": {
        "psr-4": {
            "MVC\\": "./",
            "Controllers\\": "./controllers",
            "Model\\": "./models",
            "Classes\\": "./classes",
            "CoinbaseCommerce\\": "./cbs"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "CoinbaseCommerce\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ZeNix",
            "email": "zenix699@gmail.com"
        }
    ],
    "require": {
        "phpmailer/phpmailer": "^6.8",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs",
        "test": "./vendor/bin/phpunit --verbose",
        "coverage": "./vendor/bin/phpunit --coverage-text"
      },
      "require-dev": {
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "3.*"
      }
}
